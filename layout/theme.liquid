<!DOCTYPE html>
<html
  data-wf-page="{{ wfPage }}"
  data-wf-site="63eb7019ef27e0a8a9b13b36"
  lang="{{ request.locale.iso_code }}"
  data-money-format="{{ shop.money_format | escape }}">
  <head>{%- liquid
    if headerSnippetPath
        include headerSnippetPath
    else
        render "__default-head"
    endif
-%}
    {{ content_for_header }}
    {% if template contains 'customers' %}
      {{ "shopify_common.js" | shopify_asset_url | script_tag }}
    {% endif %}

    <style>
      @font-face {
        font-family: 'Gotham';
        src: url('{{'gotham-book-font.woff2' | asset_url }}') format('woff2');
        font-weight: 400;
        font-style: normal;
        font-display: swap;
      }
      @font-face {
        font-family: 'Dreamfully';
        src: url('{{'bjola.woff2' | asset_url }}') format('woff2');
        font-weight: 400;
        font-style: normal;
        font-display: swap;
      }
      @font-face {
        font-family: 'Gotham';
        src: url('{{'fontsfree-net-gotham-bold.woff2' | asset_url }}') format('woff2');
        font-weight: 700;
        font-style: normal;
        font-display: swap;
      }
    </style>
    <!-- Google Tag Manager -->
<script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
})(window,document,'script','dataLayer','GTM-WCZTNRZ');</script>
<!-- End Google Tag Manager -->
    {% render 'analyzify-product-datalayer.liquid' %}
  </head>
  <body class="{{ bodyClasses }} {{ "template-" | append: request.page_type | handle }}">
    {% section 'announcment-bar' %}
    {% section 'header' %}
    <main
      id="main"
      class="main"
      role="main"
      tabindex="-1">
      {{ content_for_layout }}
    </main>
    {% section 'footer' %}
    {% section 'sidecart' %}
    <script
      src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=63eb7019ef27e0a8a9b13b36"
      type="text/javascript"
      integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
      crossorigin="anonymous"></script>
    <script src="{{ 'webflow.js' | asset_url }}" type="text/javascript"></script>
    <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->
    <script src="https://cdn.jsdelivr.net/npm/swiper@8/swiper-bundle.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@splidejs/splide@4.1.4/dist/js/splide.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/waypoints/4.0.0/jquery.waypoints.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery.counterup@2.1.0/jquery.counterup.min.js"></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.min.js"
      integrity="sha384-IDwe1+LCz02ROU9k972gdyvl+AESN10+x7tBKgc9I5HFtuNz0wWnPclzo6p9vxnk"
      crossorigin="anonymous"></script>
    <script
      type="text/javascript"
      src="//widget.trustpilot.com/bootstrap/v5/tp.widget.bootstrap.min.js"
      async></script>
    {% render 'scripts' %}

    <script type="application/json" data-ajax-cart-initial-state>
      {{ cart | json }}
    </script>

    <script type="module">
      import '{{ "liquid-ajax-cart-v1.10.3.js" | asset_url }}';
    </script>
    <script type="application/json" data-ajax-cart-configuration>
      {
      "addToCartCssClass": "js-my-cart-open"
                                                       }</script>


    <script type="module">
      import { subscribeToCartSectionsUpdate } from '{{ "liquid-ajax-cart-v1.10.3.js" | asset_url }}'
      const checkUpsellItem = () => {
                                                          // Get the upsell title
                                                          var upsellTitle = $('.sidecart-upsell_title').text().trim();
                                                            
                                                          // Check if there is any item with matching title
                                                          var hasMatchingItem = false;
                                                          $('.item-title-slidecart').each(function() {
                                                            if ($(this).text().trim() === upsellTitle) {
                                                              hasMatchingItem = true;
                                                              return false; // Stop iterating
                                                            }
                                                          });
                                                            
                                                          // Show/hide elements based on the matching item
                                                          if (hasMatchingItem) {
                                                            $('.upsell-heading, .sidecart-upsell_wrappepr').hide();
                                                          } else {
                                                            $('.upsell-heading, .sidecart-upsell_wrappepr').show();
                                                          }
                                                        };
                                                        
                                                        // Call the function on page load
                                                        checkUpsellItem();
                                                        
                                                        // Subscribe to cart updates
                                                        subscribeToCartSectionsUpdate(checkUpsellItem);
    </script>


    <script type="module">
      import { subscribeToCartSectionsUpdate } from '{{ "liquid-ajax-cart-v1.10.3.js" | asset_url }}'
      const checkCartUpsellItem = () => {
                                                          // Get the upsell title
                                                          var cartUpsellTitle = $('.main-cart-upsell_title').text().trim();
                                                            
                                                          // Check if there is any item with matching title
                                                          var CartHasMatchingItem = false;
                                                          $('.cart-title').each(function() {
                                                            if ($(this).text().trim() === cartUpsellTitle) {
                                                              CartHasMatchingItem = true;
                                                              return false; // Stop iterating
                                                            }
                                                          });
                                                            
                                                          // Show/hide elements based on the matching item
                                                          if (CartHasMatchingItem) {
                                                            $('.cart-upsell_wrapper').hide();
                                                          } else {
                                                            $('.cart-upsell_wrapper').show();
                                                          }
                                                        };
                                                        
                                                        // Call the function on page load
                                                        checkCartUpsellItem();
                                                        
                                                        // Subscribe to cart updates
                                                        subscribeToCartSectionsUpdate(checkCartUpsellItem);
    </script>

  </body>
</html>